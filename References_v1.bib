	Mesh = {Adenocarcinoma; CDC2-CDC28 Kinases; Cyclin-Dependent Kinase 2; Cyclin-Dependent Kinase 4; Cyclin-Dependent Kinase Inhibitor p16; Cyclin-Dependent Kinases; DNA Probes; Dose-Response Relationship, Drug; Gene Expression Regulation, Neoplastic; Genes, p16; Humans; Kinetics; Male; Metribolone; Prostatic Neoplasms; Protein-Serine-Threonine Kinases; Proto-Oncogene Proteins; Testosterone Congeners; Tumor Cells, Cultured},
	Month = {Oct},
