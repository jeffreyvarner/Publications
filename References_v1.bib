	Journal = {Cancer Res},
	Journal-Full = {Cancer research},
	Mesh = {Acetylation; Androgen Receptor Antagonists; Androgens; Cell Line, Tumor; Chromatin; Chromatin Immunoprecipitation; Dihydrotestosterone; Histones; Humans; Immunoglobulin Fragments; Male; Prostate-Specific Antigen; Prostatic Neoplasms; Quinazolines; Receptor, Epidermal Growth Factor; Receptor, erbB-2; Receptors, Androgen; Response Elements; Transcription, Genetic},
